{
  "manifest_version": 3,
  "name": "My Chrome Extension",
  "version": "1.0",
  "description": "A simple Chrome extension that transforms and reloads URLs of open tabs.",
  "permissions": [
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_title": "Transform URL"
  },
  "background": {
    "service_worker": "src/background.js",
    "type": "module"
  }
}